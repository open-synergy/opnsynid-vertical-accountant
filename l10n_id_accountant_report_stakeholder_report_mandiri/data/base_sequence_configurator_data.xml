<openerp>
<data>

<record id="base_sequence_stakeholder_mandiri" model="base.sequence_configurator">
    <field
                name="model_id"
                ref="l10n_id_accountant_report_stakeholder_report_mandiri.model_accountant_report_stakeholder_report_mandiri"
            />
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','accountant.report_stakeholder_report_mandiri'),('name','=','name')]"
            />
    <field
                name="fallback_sequence_id"
                ref="l10n_id_accountant_report_stakeholder_report_mandiri.sequence_accountant_report_stakeholder_report_mandiri"
            />
</record>

<record
            id="base_sequence_stakeholder_mandiri_line"
            model="base.sequence_configurator_line"
        >
    <field
                name="generator_id"
                ref="l10n_id_accountant_report_stakeholder_report_mandiri.base_sequence_stakeholder_mandiri"
            />
    <field name="sequence" eval="1" />
    <field name="sequence_computation_code">result = False
obj_stake_holder_mandiri = env["accountant.report_stakeholder_report_type"]
criteria = [
    ("type_id", "=", document.type_id.id)
]
sequence =\
    obj_stake_holder_mandiri.search(criteria)
if sequence:
    result = sequence.sequence_id

if not result:
    result = document.type_id.sequence_id
    </field>
</record>

</data>
</openerp>
