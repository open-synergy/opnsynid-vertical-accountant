<?xml version="1.0" ?>
<openerp>
<data>
<record id="accountant_report_method_view_form" model="ir.ui.view">
    <field name="name">accountant.report_method form</field>
    <field name="model">accountant.report_method</field>
    <field name="arch" type="xml">
        <form>
            <group name="group_1" colspan="4" col="2">
                <field name="name" />
                <field name="code" />
                <field name="active" />
                <field name="description" />
            </group>
        </form>
    </field>
</record>

<record id="accountant_report_method_view_tree" model="ir.ui.view">
    <field name="name">accountant.report_method tree</field>
    <field name="model">accountant.report_method</field>
    <field name="arch" type="xml">
        <tree>
            <field name="name" />
            <field name="code" />
        </tree>
    </field>
</record>

<record id="accountant_report_method_action" model="ir.actions.act_window">
    <field name="name">Accountant Report method</field>
    <field name="res_model">accountant.report_method</field>
    <field name="view_type">form</field>
    <field name="view_mode">tree,form</field>
</record>

<menuitem
            id="accountant_report_method_menu"
            name="Accountant Report Method"
            parent="accountant_app.accountant_configuration_menu"
            action="accountant_report_method_action"
            sequence="1"
        />
</data>
</openerp>
