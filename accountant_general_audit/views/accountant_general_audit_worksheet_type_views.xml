<openerp>
<data>

    <record id="accountant_general_audit_worksheet_type_view_search" model="ir.ui.view">
        <field name="name">accountant.general_audit_worksheet_type form</field>
        <field name="model">accountant.general_audit_worksheet_type</field>
        <field name="arch" type="xml">
            <search>
                <field name="name" />
                <field name="code" />
                <field name="model_id" />
                <field name="category_id" />
            </search>
        </field>
    </record>

    <record id="accountant_general_audit_worksheet_type_view_tree" model="ir.ui.view">
        <field name="name">accountant.general_audit_worksheet_type tree</field>
        <field name="model">accountant.general_audit_worksheet_type</field>
        <field name="arch" type="xml">
            <tree create="false" edit="false" delete="false">
                <field name="sequence" widget="handle" />
                <field name="name" />
                <field name="code" />
                <field name="category_id" />
                <field name="model_id" />
            </tree>
        </field>
    </record>

    <record id="accountant_general_audit_worksheet_type_view_form" model="ir.ui.view">
        <field name="name">accountant.general_audit_worksheet_type form</field>
        <field name="model">accountant.general_audit_worksheet_type</field>
        <field name="arch" type="xml">
            <form create="false" edit="false" delete="false">
                <header />
                <sheet>
                    <div
                            class="oe_right oe_button_box"
                            style="width: 300px;"
                            name="buttons"
                        />
                    <group name="group_1" colspan="4" col="2">
                        <field name="name" />
                        <field name="code" />
                        <field name="active" />
                        <field name="sequence" />
                        <field name="parent_id" />
                        <field name="category_id" />
                        <field name="max_number_allowed" />
                        <field name="model_id" />
                        <field name="model" />
                    </group>
                    <notebook>
                        <page name="description" string="Description">
                            <field name="description" />
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <record
            id="accountant_general_audit_worksheet_type_action"
            model="ir.actions.act_window"
        >
        <field name="name">Worksheet Types</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">accountant.general_audit_worksheet_type</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem
            id="accountant_general_audit_worksheet_type_menu"
            name="Types"
            parent="accountant_configuration_worksheet_menu"
            action="accountant_general_audit_worksheet_type_action"
            sequence="2"
        />


</data>
</openerp>
