<openerp>
<data>
    <record id="accountant_client_account_type_set_view_search" model="ir.ui.view">
        <field name="name">accountant.client_account_type_set form</field>
        <field name="model">accountant.client_account_type_set</field>
        <field name="arch" type="xml">
            <search>
                <field name="name" />
                <field name="code" />
            </search>
        </field>
    </record>

    <record id="accountant_client_account_type_set_view_tree" model="ir.ui.view">
        <field name="name">accountant.client_account_type_set tree</field>
        <field name="model">accountant.client_account_type_set</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="code" />
            </tree>
        </field>
    </record>

    <record id="accountant_client_account_type_set_view_form" model="ir.ui.view">
        <field name="name">accountant.client_account_type_set form</field>
        <field name="model">accountant.client_account_type_set</field>
        <field name="arch" type="xml">
            <form>
                <header />
                <sheet>
                    <div
                            class="oe_right oe_button_box"
                            style="width: 300px;"
                            name="buttons"
                        />
                    <group name="group_1" colspan="4" col="2">
                        <field name="name" />
                        <field name="code" />
                        <field name="active" />
                    </group>
                    <notebook>
                        <page name="account_type_set" string="Account Types">
                            <group
                                    name="account_type"
                                    string="Account Type"
                                    colspan="4"
                                    col="2"
                                >
                                <field name="detail_ids" nolabel="1" colspan="2" />
                            </group>
                        </page>
                        <page string="Sequence" name="sequence">
                            <group name="sequence_1" colspan="4" col="2">
                                <field
                                        name="accountant_general_audit_sequence_id"
                                        string="General Audit"
                                    />
                                <field
                                        name="trial_balance_sequence_id"
                                        string="Trial Balance"
                                    />
                                <field
                                        name="adjustment_entry_sequence_id"
                                        string="Adjustment Entry"
                                    />
                            </group>
                        </page>
                        <page string="Computation" name="computation">
                            <field
                                    name="computation_ids"
                                    colspan="1"
                                    col="2"
                                    nolabel="1"
                                >
                                <tree editable="bottom">
                                    <!-- <field name="account_type_set_id" /> -->
                                    <field name="computation_id" />
                                    <field name="phyton_code" />
                                </tree>
                            </field>
                        </page>
                        <page name="page_workflow" string="Workflow">
                            <group
                                    name="workflow_trial_balance"
                                    string="Trial Balance"
                                    colspan="4"
                                    col="2"
                                >
                                <field
                                        name="accountant_type_set_confirm_grp_ids"
                                        widget="many2many_tags"
                                    />
                                <field
                                        name="accountant_type_set_restart_approval_grp_ids"
                                        widget="many2many_tags"
                                    />
                                <field
                                        name="accountant_type_set_cancel_grp_ids"
                                        widget="many2many_tags"
                                    />
                                <field
                                        name="accountant_type_set_restart_grp_ids"
                                        widget="many2many_tags"
                                    />
                            </group>
                            <group
                                    name="workflow_general_audit"
                                    string="General Audit"
                                    colspan="4"
                                    col="2"
                                >
                                <field
                                        name="accountant_general_audit_confirm_grp_ids"
                                        widget="many2many_tags"
                                    />
                                <field
                                        name="accountant_general_audit_restart_validation_grp_ids"
                                        widget="many2many_tags"
                                    />
                                <field
                                        name="accountant_general_audit_cancel_grp_ids"
                                        widget="many2many_tags"
                                    />
                                <field
                                        name="accountant_general_audit_restart_grp_ids"
                                        widget="many2many_tags"
                                    />
                            </group>
                        </page>
                        <page name="description" string="Description">
                            <field name="description" />
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <record
            id="accountant_client_account_type_set_action"
            model="ir.actions.act_window"
        >
        <field name="name">Account Type Sets</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">accountant.client_account_type_set</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem
            id="accountant_client_account_type_set_menu"
            name="Account Type Sets"
            parent="accountant_configuration_trial_balance_menu"
            action="accountant_client_account_type_set_action"
            sequence="5"
        />

</data>
</openerp>
