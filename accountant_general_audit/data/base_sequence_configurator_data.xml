<openerp>
<data>
    <!-- Trial Balance -->
    <record id="sequence_config_trial_balance" model="base.sequence_configurator">
        <field
                name="model_id"
                ref="accountant_general_audit.model_accountant_client_trial_balance"
            />
        <field
                name="sequence_field_id"
                search="[('model_id.model','=','accountant.client_trial_balance'),('name','=','name')]"
            />
        <field
                name="fallback_sequence_id"
                ref="accountant_general_audit.sequence_accountant_client_trial_balance"
            />
    </record>

    <record
            id="sequence_config_line_trial_balance"
            model="base.sequence_configurator_line"
        >
        <field name="generator_id" ref="sequence_config_trial_balance" />
        <field name="sequence" eval="1" />
        <field
                name="sequence_computation_code"
            >result = document.account_type_set_id.trial_balance_sequence_id</field>
    </record>

    <!-- General Audit -->
    <record id="sequence_config_general_audit" model="base.sequence_configurator">
        <field
                name="model_id"
                ref="accountant_general_audit.model_accountant_general_audit"
            />
        <field
                name="sequence_field_id"
                search="[('model_id.model','=','accountant.general_audit'),('name','=','name')]"
            />
        <field
                name="fallback_sequence_id"
                ref="accountant_general_audit.sequence_accountant_general_audit"
            />
    </record>

    <record
            id="sequence_config_line_general_audit"
            model="base.sequence_configurator_line"
        >
        <field name="generator_id" ref="sequence_config_general_audit" />
        <field name="sequence" eval="1" />
        <field
                name="sequence_computation_code"
            >result = document.account_type_set_id.accountant_general_audit_sequence_id</field>
    </record>

    <!-- Adjustment Entry -->
    <record id="sequence_config_adjustment_entry" model="base.sequence_configurator">
        <field
                name="model_id"
                ref="accountant_general_audit.model_accountant_client_adjustment_entry"
            />
        <field
                name="sequence_field_id"
                search="[('model_id.model','=','accountant.client_adjustment_entry'),('name','=','name')]"
            />
        <field
                name="fallback_sequence_id"
                ref="accountant_general_audit.sequence_accountant_adjustment_entry"
            />
    </record>

    <record
            id="sequence_config_line_adjustment_entry"
            model="base.sequence_configurator_line"
        >
        <field name="generator_id" ref="sequence_config_adjustment_entry" />
        <field name="sequence" eval="1" />
        <field
                name="sequence_computation_code"
            >result = document.account_type_set_id.adjustment_entry_sequence_id</field>
    </record>
</data>
</openerp>
